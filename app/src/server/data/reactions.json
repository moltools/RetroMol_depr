    },
    {
        "name": "tetramate 1",
        "pattern" : "[*:1]-[C:2](-[OH:3])=[C:4]1[C:5](=[O:6])[N:7][C:8][C:9]1=[O:10]>>[*:1]-[C:2](=[OH0:3])-[CH2:4][C:5](=[O:6])[N:7][C:8][C:9](-[OH])=[O:10]"
    },
    {
        "name": "tetramate 2",
        "pattern" : "[*:1]-[C:2](-[OH:3])-[C:4]=1[C:5](=[O:6])[N:7][C:8][C:9]1-[OH:10]>>[*:1]-[C:2](=[OH0:3])-[CH2:4][C:5](=[O:6])[N:7][C:8][C:9](-[OH])=[O:10]"
    },
    {
        "name": "tetronate 1",
        "pattern" : "[*:1]-[C:2](-[OH:3])=[C:4]1[C:5](=[O:6])[O:7][C:8][C:9]1=[O:10]>>[*:1]-[C:2](=[OH0:3])-[CH2:4][C:5](=[O:6])[O:7][C:8][C:9](-[OH])=[O:10]"
    },
    {
        "name": "tetronate 2",
        "pattern" : "[*:1]-[C:2](-[OH:3])-[C:4]=1[C:5](=[O:6])[O:7][C:8][C:9]1-[OH:10]>>[*:1]-[C:2](=[OH0:3])-[CH2:4][C:5](=[O:6])[O:7][C:8][C:9](-[OH])=[O:10]"
    },
    {
        "name": "dies-alder",
        "pattern": "[C:1]-1-[C:2]=[C:3]-[C:4]-[C:5]-[C:6]-1>>([C:1]=[C:2]-[C:3]=[C:4].[C:5]=[C:6])"
    },
    {
        "name": "amino acid reduction",
        "pattern": "[NH1:1]-[CH1:2]-[CH2:3]-[OH:4]>>[NH1:1]-[CH1:2]-[CH0:3](=[O])-[OH:4]"
    },
    {
        "name": "aromatization",
        "pattern": "[*:1][c:2]1[c:3][c:4][c:5][c:6][c:7]1[*:8]>>[*:1]-[C:2]=[C:3]-[C:4]=[C:5]-[C:6]=[C:7]-[*:8]"
    },
    {
        "name": "carbamic acid transferase 1",
        "pattern": "[NH2:1]-[CH0:2](=[O:3])-[OH0:4]-[*:5]>>[NH2:1]-[CH0:2](=[O:3])[OH].[OH1:4]-[*:5]"
    },
    {
        "name": "carbamic acid transferase 2",
        "pattern": "[NH2:1]-[CH0:2](=[O:3])-[NH1:4]-[*:5]>>[NH2:1]-[CH0:2](=[O:3])[OH].[NH2:4]-[*:5]"
    },
    {
        "name": "acetic acid transferase",
        "pattern": "[CH3:1]-[CH0:2](=[O:3])-[OH0:4]-[*:5]>>[CH3:1]-[CH0:2](=[O:3])[OH].[OH1:4]-[*:5]"
    },
    {
        "name": "kirromycin-like",
        "pattern": "[c:1]1([C:2](=[O:8])[*:9])[c:3](~[O:10])[c:4][c:5][n:6][c:7](~[O:11])1>>[CH0:3](=[O:10])(-[OH])[CH2:4][CH2:5][NH1:6][CH0:7](=[O:11])[CH2:1]([CH0:2](=[O:8])[*:9])"
    },
    {
        "name": "dioxane etherification",
        "pattern": "[*:6][C:1]1[C:2][C:3]([*:7])[O:4][CH2][O:5]1>>[*:6][C:1]([OH:5])[C:2][C:3]([OH:4])[*:7]"
    },
    {
        "name": "amidotransferase",
        "pattern": "[NH1:1][CH1:2]([C:3](=[O:4])[NH2:5])[CH2:6][NH2:7]>>[NH1:1][CH1:2]([C:3](=[O:4])[OH:5])[CH2:6][NH2:7]"
    }
]