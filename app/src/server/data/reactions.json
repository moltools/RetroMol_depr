    },
    {
        "name": "carbocyclization 2",
        "pattern": "[*:1][C:2]1[C:3]([*:4])[C:5]=[C:6][CH1:7]([CH1;R:8])[C:9]([C;R:10])1>>([*:1][C:2]=1.[C:3]([*:4])[C:5]=[C:6][CH1:7](=[CH0;R:8]).[C:9]([C;R:10])=1)"
    },
    {
        "name": "oxazole",
        "pattern" : "[C:1][c:2]1[o:3][c:4][c:5]([C:6])[n:7]1>>([C:1]-[CH0:2]-1(=O).[OH1:3]-[CH2:4]-[CH1:5](-[C:6])-[NH1:7]-1)"
    },
    {
        "name": "oxazoline",
