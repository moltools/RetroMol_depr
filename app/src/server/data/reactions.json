    },
    {
        "name": "oxidation 1",
        "pattern": "[NH2:1][CH0:2]([OH:3])[C:4](=[O:5])[OH:6]>>[NH2:1][CH1:2][C:4](=[O:5])[OH:6].[OH2:3]"
    },
    {
        "name": "oxidation 2",
        "pattern": "[NH2:1][CH0:2]([OH:3])[CH2:4][C:5](=[O:6])[OH:7]>>[NH2:1][CH1:2][CH2:4][C:5](=[O:6])[OH:7].[OH2:3]"
    },
    {
        "name": "epoxidation",
        "pattern": "[C:1]1[O:2][C:3]1>>[C:1]=[C:3].[O:2]"
    },
    {
        "name": "halogenation",
        "pattern": "[C:1][Cl,Br,I:2]>>[C:1].[Cl,Br,I:2]"
    },
    {
        "name": "macrocyclization (lactone)",
