    },
    {
        "name": "thiazoline",
        "pattern" : "[C,O:1][C:2]=1[SH0:3][C:4][C:5][N:7]1>>([C,O:1]-[CH0:2]-1(=O).[SH1:3]-[CH2:4]-[C:5]-[NH1:7]-1)"
    },
    {
        "name": "thiazoline (saturated)",
