[
    { 
        "name": "PKS 1",
        "pattern": "[C,c:1][C;!R:2]~[C;!R:3][C:4](=[O:5])[OH:6]>>[C:1]C(=O)[OH].[OH][S][C:2]~[C:3][C:4](=[O:5])[OH:6]" 
    },
    {
        "name": "PKS 2",
        "pattern": "[CH0;!R:1]=[C;!R:2]-[CH1;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH1;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH0;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 3",
        "pattern": "[CH1;!R:1]=[C;!R:2]-[CH1;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH2;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH0;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 4",
        "pattern": "[CH2;!R:1]=[C;!R:2]-[CH1;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH3;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH0;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 5",
        "pattern": "[CH0;!R:1]=[C;!R:2]-[CH2;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH1;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH1;!R:3][C;!R:4](=[O:5])[OH:6]"   
    },
    {
        "name": "PKS 6",
        "pattern": "[CH0;!R:1]=[C;!R:2]-[CH2;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH1;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH1;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 7",
        "pattern": "[CH1;!R:1]=[C;!R:2]-[CH2;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH2;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH1;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 8",
        "pattern": "[CH1;!R:1]=[C;!R:2]-[CH2;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH2;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH1;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 9",
        "pattern": "[CH2;!R:1]=[C;!R:2]-[CH2;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH3;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH1;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 10",
        "pattern": "[CH1;!R:1]=[C;!R:2]-[CH0;!R:3][C;!R:4](=[O:5])[OH:6]>>[CH2;!R:1]-C(=O)[OH].[OH][S][C;!R:2]=[CH0;!R:3][C;!R:4](=[O:5])[OH:6]"
    },
    {
        "name": "PKS 11 (backwards shift)",
        "pattern": "[CH2;!R:1]-[CH1;!R:2]=[CH1;!R:3]-[CH0;!R:4][C;!R:5](=[O:6])[OH:7]>>[CH1;!R:1]=[CH1;!R:2]-C(=O)[OH].[OH][S][CH2;!R:3]-[CH0;!R:4][C;!R:5](=[O:6])[OH:7]"
    },
    {
        "name": "adenylation domain 1 (alpha)",
        "pattern": "[*:1][C:2](=[O:3])[NH1:4][C:5][C:6](=[O:7])[OH:8]>>[C:1][C:2](=[O:3])[OH].[NH2:4][C:5][C:6](=[O:7])[OH:8]"
    },
    {
        "name": "adenylation domain 2 (alpha)",
        "pattern": "[*:1][C:2](=[O:3])[NH0:4][C:5][C:6](=[O:7])[OH:8]>>[C:1][C:2](=[O:3])[OH].[NH1:4][C:5][C:6](=[O:7])[OH:8]"
    },
    {
        "name": "adenylation domain 3 (beta)",
        "pattern": "[*:1][C:2](=[O:3])[NH0:4][C:5][C:9][C:6](=[O:7])[OH:8]>>[C:1][C:2](=[O:3])[OH].[NH1:4][C:5][C:9][C:6](=[O:7])[OH:8]"
    },
    {
        "name": "disulfide linkage",
        "pattern": "[C:1][S:2][S:3][C:4]>>[C:1][SH:2].[SH:3][C:4]"
    },
    {
        "name": "sulfonation",
        "pattern": "[*:1][S:2](=[O:3])(=[O:4])[OH:5]>>[*:1].[OH][S:2](=[O:3])(=[O:4])[OH:5]"
    },
    {
        "name": "glycosyltransferase",
        "pattern": "[C:1][O:2][C:3]1[O:4][C:5][C:6][C:7][C:8]1>>[C:1][OH:2].[OH][C:3]1[O:4][C:5][C:6][C:7][C:8]1"
    },
    {
        "name": "etherification 1 (6-ring)",
        "pattern": "[*:1]-[CH0:2]1(-[OH:3])-[O:4]-[CH1:5](-[*:6])-[C:7][C:8][C:9]1>>([*:1]-[CH0:2]1=[OH0:3].[OH1:4]-[CH1:5](-[*:6])-[C:7][C:8][C:9]1)"
    },
    {
        "name": "etherification 2 (6-ring)",
        "pattern": "[*&D2,D3:3]-[CH1:1]-[C&D3:2]1-[O:4]-[CH1&D3:5](-[*:6])-[C:7][C:8][C:9]1>>([*&D2,D3:3]-[CH0:1]=[C:2]1.[OH:4]-[CH1:5](-[*:6])-[C:7][C:8][C:9]1)"
    },
    {
        "name": "etherification 3 (6-ring)",
        "pattern": "[*&D2,D3:3]-[CH2:1]-[C&D3:2]1-[O:4]-[CH1&D3:5](-[*:6])-[C:7][C:8][C:9]1>>([*&D2,D3:3]-[CH1:1]=[C:2]1.[O:4]=[CH0:5](-[*:6])-[C:7][C:8][C:9]1)"
    },
    {
        "name": "etherification 4 (6-ring)",
        "pattern": "[*:1]-[CH1:2]([C:3]~[C:4]~[C:5]1)-[O:6]-[CH1:7]1-[CH2:8]-[*:9]>>[*:1]-[C:2](-[OH:6])[C:3]~[C:4]~[C:5][CH0:7]=[CH1:8]-[*:9]"
    },
    {
        "name": "etherification 5 (6-ring)",
        "pattern": "[*:1]-[CH1:2]([C:3]~[C:4]~[C:5]1)-[O:6]-[CH1:7]1-[CH1:8]-[*:9]>>[*:1]-[C:2](-[OH:6])[C:3]~[C:4]~[C:5][CH0:7]=[CH0:8]-[*:9]"
    },
    {
        "name": "etherification 6 (6-ring)",
        "pattern": "[CH2:2]([C:3]~[C:4]~[C:5]1)-[O:6]-[CH1:7]1-[CH1:8]-[*:9]>>[C:2](-[OH:6])[C:3]~[C:4]~[C:5][CH0:7]=[CH0:8]-[*:9]"
    },
    {
        "name": "etherification 7 (6-ring)",
        "pattern": "[CH2:2]([C:3]~[C:4]~[C:5]1)-[O:6]-[CH1:7]1-[CH1:8]-[*:9]>>[C:2](-[OH:6])[C:3]~[C:4]~[C:5][CH0:7]=[CH0:8]-[*:9]"
    },
    {
        "name": "etherification 8 (6-ring)",
        "pattern": "[CH2:2]([C:3]~[C:4]~[C:5]1)-[O:6]-[CH1:7]1-[CH2:8]-[*:9]>>[C:2](-[OH:6])[C:3]~[C:4]~[C:5][CH0:7]=[CH1:8]-[*:9]"
    },
    {
        "name": "etherification 9 (5-ring)",
        "pattern": "[*&D2,D3:3]-[CH1:1]-[C&D3:2]1-[O:4]-[CH1&D3:5](-[*:6])-[C:7][C:9]1>>([*&D2,D3:3]-[CH0:1]=[C:2]1.[OH:4]-[CH1:5](-[*:6])-[C:7][C:9]1)"
    },
    {
        "name": "O-methyltransferase 1",
        "pattern": "[C:1][O:2][CH3:3]>>[C:1][OH:2].[CH4:3]"
    },
    {
        "name": "N-methyltransferase 1",
        "pattern": "[C:1][NH1:2][CH3:3]>>[C:1][NH2:2].[CH4:3]"
    },
        {
        "name": "N-methyltransferase 2",
        "pattern": "[C:1][NH0:2]([C:4])[CH3:3]>>[C:1][NH1:2][C:4].[CH4:3]"
    },
    {
        "name": "S-methyltransferase",
        "pattern": "[SH1:1][CH3:2]>>[SH2:1].[CH4:2]"
    },
    {
        "name": "C-methyltransferase 1",
        "pattern": "[NH2:1][CH0:2]([CH3:3])[C:4](=[O:5])[OH:6]>>[NH2:1][CH1:2][C:4](=[O:5])[OH:6].[CH4:3]"
    },
    {
        "name": "C-methyltransferase 2",
